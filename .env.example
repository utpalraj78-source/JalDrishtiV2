# JalDrishti Environment Variables
# Copy this file to .env.local and fill in your values

# ===================
# MAPBOX
# ===================
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# ===================
# SUPABASE
# ===================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# ===================
# JALDRISHTI BACKEND API
# ===================
# Frontend uses this to authenticate with the backend
NEXT_PUBLIC_JALDRISHTI_API_KEY=your_api_key_here

# Backend uses this to validate API requests
JALDRISHTI_API_KEY=your_api_key_here

# ===================
# OPTIONAL: EXTERNAL APIS
# ===================
# SerpAPI (for search functionality in backend)
# SERPAPI_KEY=your_serpapi_key_here
